package hackerranck;

public class Day6 {


	public static String thing(String s) {
		char[] charArray = s.toCharArray();
		for(int i = 0; i < s.length(); i++){
			if(i % 2 == 0) {
				System.out.print(charArray[i]); 
			System.out.println();
			}
			
			else if (i % 2 != 0) {
				System.out.print(charArray[i]);
			}
		}
		return null;
	}
	public static void main(String[] args) {
		
		Day6.thing("Hacker");
		
		
	}
}
