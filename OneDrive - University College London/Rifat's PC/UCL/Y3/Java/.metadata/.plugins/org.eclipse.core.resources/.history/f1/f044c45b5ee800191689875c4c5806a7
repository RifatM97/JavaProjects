package module1;

public class AlgorithmControl {
	public AlgorithmControl () {}
	  
       //////////////////task1
       ////////// A method called loop will be coded to print integers
       public double loop (double x,double y,double z) {
    	   
    	   double l = for(int x;y<=8;z) { 
    	   return l;
    	   }
       }
       
       



	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=1;i<=8;i++) {
		System.out.println(i);		
		
		}
	}


}