package module1;

public class DataTypes {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		
		//// assigning a *double* variable to the value of 5.0
		double doubVar = 5.0;
		System.out.println("the value od doubVar is" +doubVar);
		
		////assigning a "float" variable to the value of 5.0
		float floatVar = 5.0;
	    int   intVar = 5.0;
	    long  longVar = 5.0;
	    byte  byteVar = 5.0;
	    System.out.println("the value od floatVar is" +floatVar);
		System.out.println("the value od intVar is" +intVar);
		System.out.println("the value od longVar is" +longVar);
		System.out.println("the value od byteVar is" +byteVar);
		
		
		
		
	}

}
